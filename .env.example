# Required: XNAT server connection details
XNAT_SERVER='https://xnat.central.org'
XNAT_USERNAME=admin
XNAT_PASSWORD=admin

# Optional: TLS verification (default: true)
XNAT_VERIFY_TLS=true

# Optional: HTTP timeouts in seconds
# Connect timeout: max time to establish connection (default: 120 = 2 minutes)
# XNAT_HTTP_CONNECT_TIMEOUT=120
# Read timeout: max time waiting for response (default: 604800 = 7 days)
# The high default accommodates large DICOM uploads/downloads
# XNAT_HTTP_READ_TIMEOUT=604800

# Optional: DICOM C-STORE defaults
# XNAT_DICOM_HOST=xnat.example.org
# XNAT_DICOM_PORT=8104
# XNAT_DICOM_CALLED_AET=XNAT
# XNAT_DICOM_CALLING_AET=XNATIO
